<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.RemoveSplashScreenTheme" parent="@style/Base.Theme.Team12">
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="StatusBarTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryDark">@color/white</item>
        <item name="android:textColorPrimary">@color/dark_green</item>
    </style>

    <style name="calo4u">
        <item name="android:textSize">40sp</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:textColor">#FFFFFF</item>
    </style>

    <style name="by_team12">
        <item name="android:textSize">25sp</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:textColor">#CFE7CB</item>
    </style>

    <style name="CustomTabTextAppearance" parent="@android:style/TextAppearance.Small">
        <item name="android:fontFamily">@font/nunito_bold</item>
        <item name="android:textAllCaps">false</item>
    </style>
</resources>